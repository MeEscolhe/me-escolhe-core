{
  "type": "object",
  "required": ["id", "title", "category", "institution", "initialDate", "finalDate"],
  "properties": 
  {
    "id": 
    {
      "type": "string",
      "example": "5f5d1fa6dccfa335d03fdd3e"
    },

    "title": 
    {
      "type": "string",
      "example": "Developer"
    },

    "category": 
    {
      "type": "string",
      "example": "Backend"
    },

    "institution": 
    {
      "type": "string",
      "example": "UFCG"
    },

    "initialDate":
    {
      "type": "string",
      "pattern": "/([0-9]{4})-(?:[0-9]{2})-([0-9]{2})/",
      "example": "2018-08-22"
    },

    "finalDate":
    {
      "type": "string",
      "pattern": "/([0-9]{4})-(?:[0-9]{2})-([0-9]{2})/",
      "example": "2019-10-23"
    }
  }
}
