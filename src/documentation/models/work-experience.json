{
  "type": "object",
  "required": ["id", "role", "institution", "initialDate", "finalDate"],
  "properties": {
    "id": {
      "type": "string",
      "example": "5f5d1fa6dccfa335d03fdd3e"
    },

    "role": {
      "type": "string",
      "example": "Developer"
    },

    "institution": {
      "type": "string",
      "example": "Microsoft"
    },

    "initialDate": {
      "type": "string",
      "pattern": "/([0-9]{4})-(?:[0-9]{2})-([0-9]{2})/",
      "example": "2018-08-22"
    },

    "finalDate": {
      "type": "string",
      "pattern": "/([0-9]{4})-(?:[0-9]{2})-([0-9]{2})/",
      "example": "2019-10-23"
    }
  }
}
